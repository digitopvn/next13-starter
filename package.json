{
	"name": "diginext13",
	"version": "4.3.0",
	"scripts": {
		"dev": "npm run clean && npm run postinstall && concurrently \"npm run set-permission\" \"nodemon\" \"npm run node-server-dev\"",
		"reset-db-local": "npx prisma db push --force-reset && npm run postinstall && npm run seed",
		"seed": "prisma db seed",
		"node-server-dev": "node server.dev",
		"build": "npm exec npm-run-all --sequential clean postinstall img build-next",
		"postinstall": "prisma generate",
		"build-next": "next build",
		"start": "node ./server.js",
		"serve": "serve ./out",
		"build-stats": "cross-env ANALYZE=true npm run build",
		"export": "next export",
		"build-prod": "run-s clean build export",
		"clean": "rimraf .next out",
		"lint": "next lint",
		"format": "next lint --fix && prettier . --write",
		"check-secret": "node .husky/check-secret.js",
		"check-types": "tsc --project cypress --noEmit --pretty",
		"test": "jest",
		"commit": "cz",
		"cypress": "cypress open",
		"cypress:headless": "cypress run",
		"e2e": "start-server-and-test dev http://localhost:3000 cypress",
		"e2e:headless": "start-server-and-test dev http://localhost:3000 cypress:headless",
		"prepare": "husky install",
		"img": "towebp -d public/assets/images",
		"set-permission": "run-script-os",
		"set-permission:win32": "",
		"set-permission:darwin:linux": "chmod -R +x ./.husky",
		"set-permission:default": "chmod -R +x ./.husky",
		"postbuild": "next-sitemap"
	},
	"prisma": {
		"seed": "ts-node -r tsconfig-paths/register --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
	},
	"nodemonConfig": {
		"watch": [
			"public/assets/images"
		],
		"exec": "npm run img",
		"ext": "png,jpg,jpeg,gif",
		"ignore": [
			"node_modules",
			"dist",
			"build",
			"esm",
			"*.log"
		]
	},
	"dependencies": {
		"@ant-design/icons": "^5.2.6",
		"@next-auth/prisma-adapter": "^1.0.7",
		"@prisma/client": "^5.5.2",
		"@react-spring/web": "^9.7.3",
		"@styled-jsx/plugin-sass": "^4.1.0",
		"@t3-oss/env-nextjs": "^0.7.1",
		"@tanstack/react-query": "^4.29.25",
		"@tanstack/react-query-devtools": "^4.32.6",
		"@trpc/client": "^10.43.3",
		"@trpc/next": "^10.43.3",
		"@trpc/react-query": "^10.43.3",
		"@trpc/server": "^10.43.3",
		"ahooks": "^3.7.8",
		"antd": "^5.11.1",
		"axios": "^1.6.1",
		"chalk": "^5.3.0",
		"cors": "^2.8.5",
		"daisyui": "^4.0.3",
		"diginext-utils": "^3.4.0",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"form-data": "^4.0.0",
		"formidable": "^3.5.1",
		"gsap": "^3.12.2",
		"lodash": "^4.17.21",
		"net": "^1.0.2",
		"next": "^13.4.10",
		"next-auth": "^4.22.1",
		"next-seo": "^6.1.0",
		"next-sitemap": "^4.1.8",
		"npm-run-all": "^4.1.5",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-icons": "^4.11.0",
		"sass": "^1.69.5",
		"superjson": "^2.2.1",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@babel/core": "^7.23.3",
		"@commitlint/cli": "^18.4.1",
		"@commitlint/config-conventional": "^18.4.0",
		"@commitlint/cz-commitlint": "^18.4.1",
		"@next/bundle-analyzer": "^14.0.2",
		"@percy/cli": "^1.27.4",
		"@percy/cypress": "^3.1.2",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/git": "^10.0.1",
		"@testing-library/cypress": "^10.0.1",
		"@testing-library/jest-dom": "^6.1.4",
		"@testing-library/react": "^14.1.0",
		"@types/eslint": "^8.44.7",
		"@types/formidable": "^3.4.5",
		"@types/jest": "^29.5.8",
		"@types/lodash": "^4.14.201",
		"@types/node": "^20.9.0",
		"@types/prettier": "^2.7.3",
		"@types/react": "^18.2.37",
		"@types/react-dom": "^18.2.15",
		"@typescript-eslint/eslint-plugin": "^6.10.0",
		"@typescript-eslint/parser": "^6.10.0",
		"autoprefixer": "^10.4.16",
		"bson": "^6.2.0",
		"commitizen": "^4.3.0",
		"concurrently": "^8.2.2",
		"cross-env": "^7.0.3",
		"cssnano": "^6.0.1",
		"cypress": "^13.5.0",
		"diginext-img-magic-cli": "0.0.12",
		"eslint": "^8.53.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-airbnb-typescript": "^17.1.0",
		"eslint-config-next": "^14.0.2",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-cypress": "^2.15.1",
		"eslint-plugin-import": "^2.29.0",
		"eslint-plugin-jest": "^27.6.0",
		"eslint-plugin-jest-dom": "^5.1.0",
		"eslint-plugin-jest-formatting": "^3.1.0",
		"eslint-plugin-jsx-a11y": "^6.8.0",
		"eslint-plugin-prettier": "5.0.1",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"eslint-plugin-tailwindcss": "^3.13.0",
		"eslint-plugin-testing-library": "^6.1.2",
		"eslint-plugin-unused-imports": "^3.0.0",
		"husky": "^8.0.3",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"lint-staged": "^15.1.0",
		"nodemon": "^3.0.1",
		"postcss": "^8.4.31",
		"prettier": "^3.1.0",
		"prettier-plugin-tailwindcss": "^0.5.7",
		"prisma": "^5.5.2",
		"qrcode-terminal": "^0.12.0",
		"rimraf": "^5.0.5",
		"run-script-os": "^1.1.6",
		"semantic-release": "^22.0.7",
		"serve": "^14.2.1",
		"start-server-and-test": "^2.0.2",
		"tailwindcss": "^3.3.5",
		"ts-node": "^10.9.1",
		"typescript": "^5.2.2"
	},
	"config": {
		"commitizen": {
			"path": "@commitlint/cz-commitlint"
		}
	},
	"release": {
		"branches": [
			"main"
		],
		"plugins": [
			"@semantic-release/commit-analyzer",
			"@semantic-release/release-notes-generator",
			"@semantic-release/changelog",
			[
				"@semantic-release/npm",
				{
					"npmPublish": false
				}
			],
			"@semantic-release/git",
			"@semantic-release/github"
		]
	},
	"ct3aMetadata": {
		"initVersion": "7.18.0"
	}
}
